#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/Documents/Projects/fanyi-ai/fanyi-backend-web/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.45.0_@types+node@24.10.1_async-validator@4.2.5_9d769d4830f5d40ebb137dc6d35ba363/node_modules/vitepress/bin/node_modules:/Users/apple/Documents/Projects/fanyi-ai/fanyi-backend-web/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.45.0_@types+node@24.10.1_async-validator@4.2.5_9d769d4830f5d40ebb137dc6d35ba363/node_modules/vitepress/node_modules:/Users/apple/Documents/Projects/fanyi-ai/fanyi-backend-web/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.45.0_@types+node@24.10.1_async-validator@4.2.5_9d769d4830f5d40ebb137dc6d35ba363/node_modules:/Users/apple/Documents/Projects/fanyi-ai/fanyi-backend-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/Documents/Projects/fanyi-ai/fanyi-backend-web/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.45.0_@types+node@24.10.1_async-validator@4.2.5_9d769d4830f5d40ebb137dc6d35ba363/node_modules/vitepress/bin/node_modules:/Users/apple/Documents/Projects/fanyi-ai/fanyi-backend-web/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.45.0_@types+node@24.10.1_async-validator@4.2.5_9d769d4830f5d40ebb137dc6d35ba363/node_modules/vitepress/node_modules:/Users/apple/Documents/Projects/fanyi-ai/fanyi-backend-web/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.45.0_@types+node@24.10.1_async-validator@4.2.5_9d769d4830f5d40ebb137dc6d35ba363/node_modules:/Users/apple/Documents/Projects/fanyi-ai/fanyi-backend-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
